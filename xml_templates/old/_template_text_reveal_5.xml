<?xml version="1.0" encoding="UTF-8"?><data>

	<settings autocomplete="true" resizereload="true"/>

	<events>

		<event id="reset">
			<anim animtime="0.25" type="alphaout">reveal_click1</anim>
			<enable>click1</enable>			
			<anim animtime="0.25" type="alphaout">reveal_click2</anim>
			<enable>click2</enable>			
			<anim animtime="0.25" type="alphaout">reveal_click3</anim>
			<enable>click3</enable>
			<anim animtime="0.25" type="alphaout">reveal_click4</anim>
            <enable>click4</enable>
            <anim animtime="0.25" type="alphaout">reveal_click5</anim>
            <enable>click5</enable>
        </event>
		
		<event id="showreveal">
			<anim animtime="0.3" type="alpha">reveal_[ITEM]</anim>
			<disable>[ITEM]</disable>
		</event>

	</events>

	<image id="bg" y="74"><![CDATA[lib/images/ui/color_bg/placeholder.jpg]]></image>
	
    <text class="white" id="introText" width="615" x="80" y="100"></text>

	<box id="revealHolder">
		<box class="click" event="reset,showreveal" height="160" id="click1" width="160" x="80" y="235">
			<box class="clickHotspot" height="30" width="30" x="80%" y="10%"/>	
			<text id="clickText1"></text>
		</box>
		
		<box anim="hidden" bgalpha="1" bgcol="4F2070" class="reveal" height="150" id="reveal_click1" width="430" x="80" y="440">
			<pointer class="up" colour="4F2070" width="40" x="80" y="-20"/>
			<text id="reveal1text" width="calc(100% - 40px)" x="20" y="20"></text>
		</box>
		
		<box class="click" event="reset,showreveal" height="160" id="click2" width="160" x="253" y="235">
			<box class="clickHotspot" height="30" width="30" x="80%" y="10%"/>	
			<text id="clickText2"></text>
		</box>

		<box anim="hidden" bgalpha="1" bgcol="4F2070" class="reveal" height="150" id="reveal_click2" width="460" x="125" y="440">
			<pointer class="up" colour="4F2070" width="40" x="205" y="-20"/>
			<text id="reveal2text" width="calc(100% - 40px)" x="20" y="20"></text>
		</box>
		
		<box class="click" event="reset,showreveal" height="160" id="click3" width="160" x="427" y="235">
			<box class="clickHotspot" height="30" width="30" x="80%" y="10%"/>	
			<text id="clickText3"></text>
		</box>

		<box anim="hidden" bgalpha="1" bgcol="4F2070" class="reveal" height="150" id="reveal_click3" width="460" x="280" y="440">
			<pointer class="up" colour="4F2070" width="40" x="230" y="-20"/>
			<text id="reveal3text" width="calc(100% - 40px)" x="20" y="20"></text>
		</box>

		<box class="click" event="reset,showreveal,4" height="160" id="click4" width="160" x="600" y="235">
			<box class="clickHotspot" height="30" width="30" x="80%" y="10%"/>	
			<text id="clickText4"></text>
		</box>
		
        <box anim="hidden" bgalpha="1" bgcol="4F2070" class="reveal" height="150" id="reveal_click4" width="460" x="450" y="440">
			<pointer class="up" colour="4F2070" width="40" x="230" y="-20"/>
			<text id="reveal4text" width="calc(100% - 40px)" x="20" y="20"></text>
		</box>
		
        <box class="click" event="reset,showreveal,5" height="160" id="click5" width="160" x="774" y="235">
			<box class="clickHotspot" height="30" width="30" x="80%" y="10%"/>	
			<text id="clickText5"></text>
		</box>
		
        <box anim="hidden" bgalpha="1" bgcol="4F2070" class="reveal" height="150" id="reveal_click5" width="480" x="450" y="440">
			<pointer class="up" colour="4F2070" width="40" x="410" y="-20"/>
			<text id="reveal5text" width="calc(100% - 40px)" x="20" y="20"></text>
		</box>
		
    </box>


</data>
