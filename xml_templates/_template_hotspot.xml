<?xml version="1.0" encoding="UTF-8"?><data>

	<settings autocomplete="true" resizereload="true"/>

	<events>
	
		<event id="reset">
			<anim animtime="0.25" type="alphaout">reveal_hotspot1</anim>
			<enable>hotspot1</enable>
			<anim animtime="0.25" type="alphaout">reveal_hotspot2</anim>
			<enable>hotspot2</enable>
			<anim animtime="0.25" type="alphaout">reveal_hotspot3</anim>
			<enable>hotspot3</enable>

	
		</event>

		<event id="showreveal">
			<anim animtime="0.3" type="alpha">reveal_[ITEM]</anim>
			<disable>[ITEM]</disable>
		</event>

	</events>

	<image id="bg" y="74"><![CDATA[lib/images/content/placeholder.jpg]]></image>
	
	
    <text id="screentext" width="615" x="25" y="100"></text>

	
	<box id="hotspotHolder">
		<box id="hotspot1" class="hotspot" x="195" y="335" width="30" height="30" event="reset,showreveal"/>	
		<box id="hotspot2" class="hotspot" x="295" y="335" width="30" height="30" event="reset,showreveal"/>	
		<box id="hotspot3" class="hotspot" x="395" y="335" width="30" height="30" event="reset,showreveal"/>	

	</box>
	

	<box id="revealHolder">
		<box id="reveal_hotspot1" anim="hidden" bgalpha="1" bgcol="FFFFFF" class="hotspotReveal" width="340" height="340" x="495" y="170">
			<box id="title1banner" bgcol="0036A3" width="100%" height="40">
				<text id="reveal1title" class="revealtitle" x="20" y="10"></text>		
				<svg id="closeBtn1" class="navBtn" x="calc(100% - 40px)" y="10" width="20" height="20" src="lib/images/ui/ui_reveal_close_icon.svg" event="reset"/>
			</box>
			<text id="hotspot_1_text" width="calc(100% - 40px)" x="20" y="60"></text>

		</box>
		
		<box id="reveal_hotspot2" anim="hidden" bgalpha="1" bgcol="FFFFFF" class="hotspotReveal" width="340" height="340" x="495" y="170">
			<box id="title2banner" bgcol="0036A3" width="100%" height="40">
				<text id="reveal2title" class="revealtitle" x="20" y="10"></text>		
				<svg id="closeBtn1" class="navBtn" x="calc(100% - 40px)" y="10" width="20" height="20" src="lib/images/ui/ui_reveal_close_icon.svg" event="reset"/>
			</box>
			<text id="hotspot_2_text" width="calc(100% - 40px)" x="20" y="60"></text>

		</box>

		<box id="reveal_hotspot3" anim="hidden" bgalpha="1" bgcol="FFFFFF" class="hotspotReveal" width="340" height="340" x="495" y="170">
			<box id="title3banner" bgcol="0036A3" width="100%" height="40">
				<text id="reveal3title" class="revealtitle" x="20" y="10"></text>		
				<svg id="closeBtn1" class="navBtn" x="calc(100% - 40px)" y="10" width="20" height="20" src="lib/images/ui/ui_reveal_close_icon.svg" event="reset"/>
			</box>
			<text id="hotspot_3_text" width="calc(100% - 40px)" x="20" y="60"></text>
		
		</box>

		



	</box>


</data>
