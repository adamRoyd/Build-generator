
#preloader{
	position: absolute;
	width: 100%;
	height: 100%;
}


/* NAV */

#navholder{
	/* display: none; */
	width:100%;
	top: 0px;
	z-index: 100;
	text-align: right;
	box-sizing: border-box;
	position: fixed !important;

}

.fixed{
	position: fixed !important;
}

#navholder .fixed{	
	z-index: 100 !important;
	width:100%;
	height: 50px!important;
	padding: 10px;
	background: #FFFFFF;
	border-bottom: 1px solid #E1E1E1;
	margin:  0;
	width: 100% !important;
}

.screenElement.text.topictitle{
	top: 50px !important;
	position: fixed !important;
	width: 100% !important;
	background: rgba(255,255,255,0.9);
	border-bottom:  1px solid white;
	padding: 5px;
}

#navholder .logo{
	-ms-transform: scale(0.69,0.69) !important;
    -ms-transform-origin: 0 0 !important;
	-webkit-transform: scale(0.69,0.69) !important;
    -webkit-transform-origin: 0 0 !important;
	transform: scale(0.69,0.69) !important;
    transform-origin: 0 0 !important;
	display: inline-block !important;
	background: url('../../images/ui/ui_spritesheet.png') no-repeat -544px -650px;
	width: 145px !important;
	height: 30px !important;
	float: left;
	margin-left: 115px;
	margin-top: 5px;
	/* margin: 10px; */
}

#navholder .logo .after{
	background: url('../../images/ui/ui_spritesheet.png') no-repeat -280px -657px;
	width: 167px;
	height: 16px;
	position: absolute;
	left: -170px;
}

.in-topic #navholder .logo{
	background-position: -555px -650px;
	margin-left: 0px;
}

.in-topic #navholder .logo .after{
	display: none;
}

div.box.burgerBtn{
	display: inline-block !important;
	/* position: absolute !important; */
	width: 31px !important;
	height: 30px !important;
	margin-left: 5px;
}

.burgerBtn:after{
  content: "\f0c9";
  font: normal normal normal 14px/1 FontAwesome;
  font-size: 32px;
  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  transform: translate(0, 0);
}

div.box.burger_menu{
	position: absolute !important;
	width: 100% !important;
	height: 1000px !important;
	display: none;
	margin-top: 50px;
	z-index: 50 !important;
}

div.box.scores{
	height: 90px !important;
	text-align: right;
	margin-top: 0px;
}

.screenElement.score_categories{
	float: right;
	width: 55% !important;
	margin: 10px 10px 0 0;
}

div.box.score_stars{
	width: 110px !important;
	float: left !important;
	margin: 30px 0 0 10px;
}

div.box.score_stars .text{
	width: 100% !important;
}

div.box.score_stars p{
	text-align: center !important;
	width: 100% !important;
}

.scores>.box {
	/* width: 70% !important; */
}
.scores>.box .score{
	width: 30% !important;
	display: inline-block !important;
}
.scores>.box .text{
	display: inline-block !important;
}

.scores p{
	text-align: left !important;
	font-weight: bold;
	font-size: 15px;
	margin:0;
}
.scores .status p{
	text-align: left !important;
}

.score{
	background: white;
}

.score .bar{
	position: absolute;
	background: #C60651;
	height: 100%;
	width: 0%;
}
.scores>.box .score.stars {
	width: 110px !important;
	background: url('../../images/ui/ui_spritesheet.png') no-repeat -10px -850px;
}
.score.stars .bar{
	position: absolute;
	background: url('../../images/ui/ui_spritesheet.png') no-repeat -10px -1060px transparent;
	height: 100%;
	width: 0%;
}

.menuitems{
	clear: both;
}

.menuitems .text.disabled{
	display: none !important;
}

.menuitems .text {
	background: white;
	padding: 20px !important;
	border-bottom: 1px solid silver;
}

.menuitems .text p{
	text-align: center;
	margin: 0;
}

.pagecounter p{
	margin-top: 0px !important;
	position: absolute;
	top: 5px;
}
.in-topic .pagecounter,
.in-topic .pagenavbuttons{
	display: inline-block !important;
}
.in-topic .pagecounter.screenElement{
	display: inline-block !important;
	width: 72px !important;
}
.in-topic .pagecounter .background{
	display: none !important;
}

.in-topic .pagenavbuttons
{
	position: absolute;
	top: 9px !important;
	right: auto;
	left: 50% !important;
	margin-left:  -70px;
	width: 140px !important;
	/* bordeR: 1px solid red; */
}

.pagenavbuttons{
	display: none !important;
}


div.box.nextBtn{
	display: inline-block !important;
	/* position: absolute !important; */
	width: 31px !important;
	height: 30px !important;
}
div.box.backBtn{
	display: inline-block !important;
	position: relative !important;
	width: 31px !important;
	height: 30px !important;
}

.nextBtn:after,
.backBtn:after
{
  content: "\f138";
  font: normal normal normal 14px/1 FontAwesome;
  font-size: 32px;
  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  transform: translate(0, 0);
}
.backBtn:after
{
  content: "\f137";
}


.BIT .navBtn{
	background: blue;
	border-radius: 5px;
	width: 50px;
	top: 0;
	left: 0;
}

#bitTxt{
	background: rgba(255, 255, 255, 0.6);
	position:absolute;
	left: 60;
}

#dialogueCloseBtn{
	background: url('../../images/ui/ui_spritesheet.png') no-repeat -1570px -430px;
	width: 30px;
	height: 30px;
	right: 10px;
	top: 10px;
}


#dialogueCloseBtn p{
	left: -60px;
	position: absolute;
}

#dialogueCloseBtn.over{
	background: url('../../images/ui/ui_spritesheet.png') no-repeat -1570px -640px;
}

